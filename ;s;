    function Dupe(amount)
        local DropT = {}
        speaker.Character.Humanoid:UnequipTools()
        stopDupe = false
        local Pos = speaker.Character.HumanoidRootPart.CFrame
        speaker.Character:MoveTo(Vector3.new(0, 1e7, 0))
        wait(.15)
        speaker.Character.HumanoidRootPart.Anchored = true
        for i = 1, amount do
            if stopDupe then
                break
            end
            wait(0.4)
            speaker.Character:MoveTo(Vector3.new(0, 1e7, 0))
            wait(.2)
            for i, v in pairs(speaker.Backpack:GetChildren()) do
                if v:IsA("Tool") and v.Name == radio then
                    v.Parent = speaker.Character
                    wait(.08)
                    v.Parent = workspace
                    table.insert(DropT, v)
                end
            end
            wait(.2)
            repeat 
                game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(";respawn ") wait()
            until speaker.CharacterAdded:wait()
            if stopDupe then
                break
            end
        end
        wait(0.2)
        speaker.Character.Humanoid:UnequipTools()
        wait(0.3)
        for i, v in pairs(Workspace:GetChildren("\216\167\217\132\216\167\216\186\216\167\217\134\217\138 \217\133\216\179\216\172\217\132")) do
        if v:IsA("Tool") and v.Name == radio then
            firetouchinterest(v.Handle, speaker.Character.RightHand, 0)
            speaker.Character.HumanoidRootPart.CFrame = Pos
        end
        end
        grabtools()
        wait(0.5)
        ungrabtools()
    wait(.4)
    end    

    function antibang()
        _G.ay = true
        local baseplate = Instance.new("Part")
        baseplate.Parent = workspace
        baseplate.Size = Vector3.new(30,1,30)
        baseplate.Anchored = true
        baseplate.Name = "Baseplate553"

        baseplate.Position = Vector3.new(0, -507.5284729003906, 0)
        killv2()  
        wait(0.2)
        while _G.ay == true do
        local Pos = speaker.Character.HumanoidRootPart.CFrame
        speaker.Character.HumanoidRootPart.CFrame = CFrame.new(0, -504.5284729003906, 0)
        wait(0.5)
        speaker.Character.HumanoidRootPart.CFrame = Pos
        wait(8)
        end
    end

    function unantibang()
        _G.ay = false
        wait(1)
        for i,v in pairs(game:GetDescendants()) do
            if v.Name == "Baseplate" then v:Destroy() end
        end
    end

    function removetools()
        speaker.Backpack("رقصة 1").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 2").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 3").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 4").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 5").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 6").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 7").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 8").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 9").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 01").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 11").Name = "قص"
        speaker.Backpack:FindFirstChild("رقصة 21").Name = "قص"
        for i,v in pairs(speaker.Backpack:GetDescendants()) do
            if v.Name ==  "قص" then
                v:Destroy()
            end
        end 
    end

    function getRoot(char)
        local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
        return rootPart
    end

    function tools(plr)
        if plr:FindFirstChildOfClass("Backpack"):FindFirstChildOfClass('Tool') or plr.Character:FindFirstChildOfClass('Tool') then
            return true
        end
    end

    function grabtools()
        local Human = speaker.Character:FindFirstChildWhichIsA("Humanoid")
        for _, v in ipairs(workspace:GetChildren()) do
            if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
                Human:EquipTool(v)
            end
        end
        if grabtoolsFunc then grabtoolsFunc:Disconnect() end
        grabtoolsFunc = workspace.ChildAdded:Connect(function(v)
            if speaker.Character and v:IsA("BackpackItem") and v:FindFirstChild("Handle") then
                speaker.Character.Humanoid:EquipTools()
            end
        end)
    end
    function ungrabtools()
        if grabtoolsFunc then grabtoolsFunc:Disconnect() end
    end
    spawn(function ()
        function dropped()
        if speaker.Character then
            for _,obj in pairs(speaker.Character:GetChildren()) do
                if obj:IsA("Tool") then
                    obj.CanBeDropped = true
                end
            end
        end
        if speaker:FindFirstChildOfClass("Backpack") then
            for _,obj in pairs(speaker:FindFirstChildOfClass("Backpack"):GetChildren()) do
                if obj:IsA("Tool") then
                    obj.CanBeDropped = true
                end
            end
        end
    end
    end)



