while wait(0.6) do
    for i,v in pairs(game:GetService("CoreGui"):GetDescendants()) do
        if v.Name == "AntiAfk" and v.ClassName == "ScreenGui" then
            v.Enabled = false
        end
    end
end
while true do 
for i,v in pairs(game:GetDescendants()) do
    if v.Name == "Baseplate553" then
    v.Color = Color3.new(255/255,0/255,0/255)
    for i = 0,255,10 do
    wait()
    v.Color = Color3.new(255/255,i/255,0/255)
    end
    for i = 255,0,-10 do
    wait()
    v.Color = Color3.new(i/255,255/255,0/255)
    end
    for i = 0,255,10 do
    wait()
    v.Color = Color3.new(0/255,255/255,i/255)
    end
    for i = 255,0,-10 do
    wait()
    v.Color = Color3.new(0/255,i/255,255/255)
    end
    for i = 0,255,10 do
    wait()
    v.Color = Color3.new(i/255,0/255,255/255)
    end
    for i = 255,0,-10 do
    wait()
    v.Color = Color3.new(255/255,0/255,i/255)
    end
    end
end
end
